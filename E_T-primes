from math import sqrt
length=int(input())
numbers=list(map(int,input().split()))
def is_prime(num):
    if num < 2:
        return False
    if num == 2:
        return True
    if num % 2 == 0:
        return False
    for i in range(3, int(sqrt(num)+1), 2):
        if num % i == 0:
            return False
    return True
for K in range(length):
    x=numbers[K]
    num=int(sqrt(x))
    if num*num==x:
        result=is_prime(num)
        if result:
            print("YES")
        else:
            print("NO")
    else:
        print("NO")
