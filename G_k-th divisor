from math import sqrt 
n,k=list(map(int,input().split()))
divisors=[]
factors=[]
for i in range(1,int(sqrt(n))+1):
    if n%i==0:
        divisors.append(i)
        if i!=n//i:
            factors.append(n//i)
factors=factors[::-1]

if len(divisors)+len(factors)>=k:
    if k<=len(divisors):
        print(divisors[k-1])
    else:
        print(factors[k-len(divisors)-1])
else:
    print("-1")

