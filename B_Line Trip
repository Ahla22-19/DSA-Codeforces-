t = int(input())
for i in range(t):
    n, x = list(map(int, input().split()))
    a = list(map(int, input().split()))

    point = [0] + a + [x]
    maxi = 0
    for i in range(1, len(point)):
        maxi = max(maxi, point[i] - point[i - 1])
    maxi = max(maxi, 2 * (x - a[-1]))
    print(maxi)
    



