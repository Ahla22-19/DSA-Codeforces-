t = int(input())

for i in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    i = 0

    while i + 1 < n:

        if a[i] == 1 and a[i+1] == 1:
            a[i] += 1
            a[i+1] += 2

        elif a[i] == 1:
            a[i] += 1
        
        if i >0 and a[i] % a[i - 1] == 0:
            a[i] += 1
        
        if a[i+1] >= a[i] and a[i+1] % a[i] == 0:
            a[i+1] += 1 

        i += 1
    
    print(*a)
