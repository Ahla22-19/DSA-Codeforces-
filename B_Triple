n=int(input())
for i in range(n):
    m=int(input())
    nums=list(map(int,input().split()))
    nums.sort()
    l=0
    r=l+1
    while r<len(nums): 
        count=1
        if r+1<len(nums) and nums[l]==nums[r] and nums[r]==nums[r+1]:
            count+=2
            l+=1
            r+=1
        else:
            l+=1
            r+=1
        if count==3:
            print(nums[l])
            break 
    else:
        print("-1")

